{
  "name": "organize-api",
  "version": "1.0.0",
  "description": "API of organize project, a tool for you to create, open and organize your projects.",
  "main": "index.js",
  "scripts": {
    "node:dev": "export NODE_ENV=development",
    "start:dev": "yarn node:dev && ts-node-dev -r tsconfig-paths/register -r dotenv/config --inspect --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts | pino-pretty -c",
    "test:integration": "jest --projects ./tests --runInBand",
    "test:unit": "jest --projects .",
    "test": "yarn lint && yarn test:unit && yarn test:integration",
    "lint": "eslint ./src ./tests --ext .ts",
    "lint:fix": "eslint ./src ./tests --ext .ts --fix",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "pm2-runtime start ecosystem.config.js --env production"
  },
  "engines": {
    "node": "16.13.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Suares01/organize-API.git"
  },
  "keywords": [
    "node",
    "organize",
    "API",
    "typescript"
  ],
  "author": "Lucas Suares",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Suares01/organize-API/issues"
  },
  "homepage": "https://github.com/Suares01/organize-API#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.41",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.3",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "16.3.1",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "pino-pretty": "^7.5.1",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "config": "^3.3.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-pino-logger": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.2",
    "pino": "^7.8.0",
    "pm2": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0"
  }
}
